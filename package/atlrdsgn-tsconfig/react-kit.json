{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Atelier.kit React",
  "extends": "./base.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "sourceMap": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "target": "es6",
    "allowSyntheticDefaultImports": true,

    "resolveJsonModule": true,
    "allowJs": true,
    "noImplicitAny": false
  }
}
