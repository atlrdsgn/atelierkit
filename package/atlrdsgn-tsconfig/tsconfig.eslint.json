{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "kit React",
  "extends": ["./base.json", "./next.json", "./react-kit.json", "./node16.json"],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "noImplicitAny": false
  }
}
